<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <staticContent>
      <remove fileExtension="cap"/>
      <mimeMap fileExtension="cap" mimeType="application/octet-stream" />
      <remove fileExtension="webmanifest"/>
      <mimeMap fileExtension="webmanifest" mimeType="application/manifest+json" />
    </staticContent>
  </system.webServer>
</configuration>